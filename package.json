{
  "name": "vite-shop",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:search": "concurrently \"npm:dev\" \"npm:search:server\" \"npm:search:client\"",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "search:client": "cd server; node meilisearch",
    "search:server": "meilisearch",
    "coverage": "vitest run --coverage",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@heroicons/vue": "^2.0.12",
    "@meilisearch/instant-meilisearch": "^0.9.0",
    "@supabase/supabase-js": "^1.35.7",
    "@vueuse/core": "^9.3.0",
    "concurrently": "^7.5.0",
    "daisyui": "^2.31.0",
    "happy-dom": "^7.5.12",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-instantsearch": "^4.6.0",
    "vue-router": "^4.1.5",
    "vue-toast-notification": "^3.0"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-c8": "^0.24.5",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.7.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-pwa": "^0.13.1",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7"
  }
}
